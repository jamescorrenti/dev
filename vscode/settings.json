    "workbench.activityBar.visible": false,
    "pythonTestExplorer.testFramework": "pytest",
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
