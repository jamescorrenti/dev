{
    /**
    * Better Defaults
    **/
    "editor.copyWithSyntaxHighlighting": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.emptySelectionClipboard": false,
    "workbench.editor.enablePreview": false,
    "window.newWindowDimensions": "inherit",
    "editor.multiCursorModifier": "ctrlCmd",
    "files.trimTrailingWhitespace": true,
    "diffEditor.renderSideBySide": false,
    "editor.snippetSuggestions": "top",
    "editor.detectIndentation": false,
    "window.nativeFullScreen": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "workbench.colorTheme": "Palenight Theme",
    "workbench.sideBar.location": "right",
    "workbench.editor.showTabs": false,
    "workbench.statusBar.visible": false,
    "editor.minimap.enabled": false,
    /**

    * Silence the Noise
    **/
    "breadcrumbs.enabled": false,
    "scm.diffDecorations": "none",
    "workbench.tips.enabled": false,
    "workbench.startupEditor": "none",
    "editor.lightbulb.enabled": false,
    /**
    * Find
    */
    "search.useIgnoreFiles": false,
    "search.exclude": {
        // Hide everything in /vendor, except the "laravel" and "livewire" folder.
        "**/vendor/{[^l],?[^ai]}*": true,
        // Hide everything in /public, except "index.php"
        "**/public/{[^i],?[^n]}*": true,
        "**/node_modules": true,
        "**/__pycache__": true,
        "**/*venv": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/_ide_helper.php": true,
        "**/composer.lock": true,
        "**/package-lock.json": true,
        "storage": true,
        "**/*.code-search": true
    },

    /**
    * Project Switcher
    */
    "simple-project-switcher.directory": "/Users/jecorren/Desktop/src",

    /**
    * Typography
    */
    "editor.fontSize": 16,
    "editor.lineHeight": 45,
    "editor.suggestFontSize": 16,
    "editor.suggestLineHeight": 28,
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.lineHeight": 1.5,

    /**
    * Disable standard Pytest
    */
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": false,
    "pythonTestExplorer.testFramework": "pytest", // (or unittest)

